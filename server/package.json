{
  "name": "swarmerai-graphql-server",
  "version": "0.1.0",
  "scripts": {
    "start": "env $(cat .env) nodemon -e js,graphql -x babel-node src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "nodemon": "nodemon",
    "deploy-prisma:local": "prisma deploy -e .env",
    "deploy-prisma-force:local": "prisma deploy -e .env --force"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^3.10.0",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.29.2",
    "prisma-client-lib": "^1.34.0"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^6.2.0",
    "nodemon": "^1.19.1",
    "prisma": "^1.34.0"
  }
}
